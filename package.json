    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.1",
    "watch": "^1.0.2",
    "webpack": "^4.17.1",
    "webpack-assets-manifest": "^3.0.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.2.0",
